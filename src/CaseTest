MODULE Primes;
(* Простые числа от 2 до n *)

IMPORT
   Out, In;

CONST
    w = 8;
VAR
   i, n, d: INTEGER;
   c: INTEGER;
BEGIN
    In.Open();
    i := 2
   CASE c + n * 2 OF
        n..n, n..n : |
        i + 4: i := n + d;
            i := n + d;
        |n, n : i := n + d;
            i := n + d;
        |n..n, n..n :

        ELSE i := n;
   END;
END Primes.
